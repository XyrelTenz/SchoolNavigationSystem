{
  "expo": {
    "name": "School Navigation System",
    "slug": "school-navigation-system",
    "version": "1.0.0",
    "icon": "./assets/SchoolLogo.png",
    "userInterfaceStyle": "light",
    "orientation": "portrait",
    "splash": {
      "image": "./assets/NavigationLogo.png",
      "resizeMode": "contain",
      "backgroundColor": "#600EE6"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/NavigationLogo.png",
        "backgroundColor": "#600EE6"
      }
    },
    "web": {
      "favicon": "./assets/SchoolLogo.png"
    },
    "experiments": {
      "tsconfigPaths": true
    },
    "plugins": [
      "expo-router"
    ]
  }
}